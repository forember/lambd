@@ include list.

@@ def %ex1.
  (@@list e.4 8 1 5 1 6)
@@ def %ex2.
  (@@list e.2 3 4 2)
  
@@ def main. (list:_print
  (@@list e.
    (list:_print (list:getSlice %ex1 2 3))
    (list:_print (list:setSlice %ex1 2 3 %ex2))
    (list:_print (list:applyAll list:getSlice
        (@@list e.%ex1 2 3)))
    (list:length %ex2)))
